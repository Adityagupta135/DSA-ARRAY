#include <iostream>
using namespace std;

void printalternate(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cout<<arr[i]<<endl;
    }
}


void alternateswap(int arr[], int n)
{
for(int i=0; i<n; i += 2)
{
    if(i<n-1)
    {
        swap(arr[i], arr[i+1]);
    }
}
}


int main()
{
    
  
 int even[6]={4,8,99,0,65,45};
 
 int odd[5]= {7,5,98,45,4};
 
 
 alternateswap(even,6);
 
 printalternate(even,6);
 
    return 0;
    
}

