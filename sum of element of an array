// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    
    int n;
    
    int arr[n];
    
    cout<<"enter the size of an array";
    
    cin>>n;
    
    for(int i=0; i<n; i++)
    
    {
        cin>>arr[i];
    }


int sum =0;

for(int i=0; i<n; i++)
{
    sum = sum+arr[i];
}

cout<<sum<<endl;
    return 0;
}
